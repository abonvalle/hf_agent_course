# Copy this file to .env and fill in your actual API keys

# OpenAI API Key (required for the LLM)
OPENAI_API_KEY=sk-your-openai-api-key-here

# Tavily API Key (required for web search)
TAVILY_API_KEY=tvly-your-tavily-api-key-here

# Hugging Face Space ID (automatically set when deployed to HF Spaces)
SPACE_ID=your-username/your-space-name

# Hugging Face Space Host (automatically set when deployed to HF Spaces)
SPACE_HOST=your-space-host.hf.space

# Optional: Customize the scoring API URL
API_URL=https://agents-course-unit4-scoring.hf.space
